request=[ 98,183,37,122,14,124,65,67]
head=53
total_movement=0
seek_sequence=[]

while request:
    request.sort(key=lambda x:abs(x-head))
    closest_request=request.pop(0)
    total_movement+=abs(head-closest_request)
    head=closest_request

    seek_sequence.append(head)
    
print("seek_sequence",seek_sequence)
print("total head movement:",total_movement)
